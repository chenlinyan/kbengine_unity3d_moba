<root>
	<Components>
		<componentFrameSync>
			<Type> 			FrameSyncReport			</Type>
			<Persistent>	true					</Persistent>
		</componentFrameSync>
		<componentMatcherAvatar>
			<Type> 			MatchAvatarReport		</Type>
			<Persistent>	true					</Persistent>
		</componentMatcherAvatar>
	</Components>

	<Properties>
		<roomKey>
			<Type>				SPACE_KEY			</Type>
			<Flags>				BASE				</Flags>
			<Persistent>		true				</Persistent>
		</roomKey>

		<roomCellEntity>
			<Type>				ENTITYCALL			</Type>
			<Flags>				CELL_PUBLIC			</Flags>
		</roomCellEntity>

		<name>
			<Type>				UNICODE				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
			<Persistent>		true				</Persistent>
			<DetailLevel>		MEDIUM				</DetailLevel>
		</name>

		<heroIdLst>
			<Type>				HERO_IDS_LIST		</Type>
			<Flags>				BASE				</Flags>
		</heroIdLst>

		<heroId>
			<Type>				HERO_ID				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
			<Default>			0					</Default>
		</heroId>

		<heroInfo>
			<Type>				HERO_INFOS			</Type>
			<Flags>				CELL_PUBLIC			</Flags>
		</heroInfo>

		<dbid>
			<Type>				DBID				</Type>
			<Flags>				CELL_PUBLIC			</Flags>
			<Default>			0					</Default>
		</dbid>

		<matchId>
			<Type>				MATCH_ID			</Type>
			<Flags>				BASE				</Flags>
			<Persistent>		true				</Persistent>
		</matchId>

		<teamId>
			<Type>				TEAM_ID 			</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</teamId>

		<bag>
			<Type>				INT8				</Type>
			<Flags>				CELL_PUBLIC			</Flags>
			<Default>			0					</Default>
		</bag>

		<winRate>
			<Type>				INT8				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</winRate>
		<roundCount>
			<Type>				INT8				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</roundCount>
		<gameStateC>
			<Type>				INT8				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</gameStateC>
		<seatNo>
			<Type>				INT8				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</seatNo>
		<level>
			<Type>				INT8				</Type>
			<Flags>				ALL_CLIENTS			</Flags>
		</level>
		<coin>
			<Type>				INT8				</Type>
			<Flags>				CELL_PUBLIC			</Flags>
		</coin>
	</Properties>

	<ClientMethods>
		<onReqsChooseHeroResult>
			<Arg>				HERO_INFOS			</Arg>
			<Arg>				SKILL_INFOS_LIST	</Arg>
		</onReqsChooseHeroResult>

		<onReqsSkillLstResult>
			<Arg>				SKILL_INFOS_LIST	</Arg>
		</onReqsSkillLstResult>

		<onPushStatisticalResult>
			<Arg>				TEAM_ID				</Arg>
		</onPushStatisticalResult>

		<onPushAvatarCurrentScene>
			<Arg>				UINT32				</Arg>
		</onPushAvatarCurrentScene>

		<onPushMatchPlayersData>
			<Arg>		MATCHING_INFOS_LIST			</Arg>
		</onPushMatchPlayersData>

		<onHeroIdChanged>
			<Arg>			ENTITY_ID				</Arg>
			<Arg>			HERO_ID					</Arg>
		</onHeroIdChanged>

		<onLoadingToReadyBattleState>
		</onLoadingToReadyBattleState>

		<onReadyBattle>
		</onReadyBattle>

		<onExitMatch>
			<Arg>			ENTITY_ID				</Arg>
		</onExitMatch>

	</ClientMethods>


	<BaseMethods>
		<startGame>
			<Exposed/>
		</startGame>

		<leaveGame>
			<Exposed/>
		</leaveGame>

		<reqHeroInfosByHeroId>
			<Exposed/>
			<Arg>				HERO_ID						</Arg>
		</reqHeroInfosByHeroId>

		<reqSkillLst>
			<Exposed/>
		</reqSkillLst>

		<reqExitMatch>
			<Exposed/>
		</reqExitMatch>

	</BaseMethods>

	<CellMethods>
		<getHeroId>
		</getHeroId>

		<getTeamId>
		</getTeamId>

		<setTeamId>
			<Arg>				TEAM_ID 					</Arg>
		</setTeamId>

		<submitStatisticalResult>
			<Exposed/>
			<Arg>				TEAM_ID						</Arg>
		</submitStatisticalResult>

	</CellMethods>
</root>
